[
  {
    "context": "Editor && !agent_diff",
    "bindings": {
      "ctrl-alt-s": "git::ToggleStaged"
    }
  },
  {
    "context": "(Editor && mode == full)",
    "bindings": {
      "ctrl-k": "assistant::InlineAssist"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "ctrl-l": null
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "ctrl-r": [
        "projects::OpenRecent",
        {
          "create_new_window": false
        }
      ]
    }
  }
]
